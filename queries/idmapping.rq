SELECT DISTINCT (substr(STR(?gene),32) AS ?wikidata) ?ncbi ?refseq ?uniprot WHERE {
  VALUES ?virus {
    wd:Q82069695 # SARS-CoV-2
    wd:Q16983360 # HKU1
    wd:Q16991954 # OC43
    wd:Q8351095  # NL63 
    wd:Q16983356 # 229E
    wd:Q4902157  # MERS-CoV
    wd:Q278567   # SARS-CoV
  }
  ?gene wdt:P703 ?virus ; wdt:P31 wd:Q7187 .
  OPTIONAL { ?gene wdt:P351 ?ncbi }
  OPTIONAL {
    ?gene wdt:P688 | ^wdt:P702 ?protein .
    OPTIONAL { ?protein wdt:P637 ?refseq }
    OPTIONAL { ?protein wdt:P352 ?uniprot }
  }
} ORDER BY ?ncbi ?wikidata